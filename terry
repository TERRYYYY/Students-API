CREATE TABLE foreigners(id INT NOT NULL AUTO_INCREMENT,
name VARCHAR(30) NOT NULL,
email VARCHAR(60) NOT NULL,
gender VARCHAR(60) NOT NULL,
status VARCHAR(60) NOT NULL,
PRIMARY KEY(id));

